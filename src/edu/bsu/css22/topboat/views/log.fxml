<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxWidth="269.0" minHeight="-Infinity" minWidth="269.0" prefWidth="269.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.bsu.css22.topboat.controllers.LogController">
    <children>
        <TabPane fx:id="logs" minHeight="-Infinity" prefWidth="269.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="45.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab fx:id="game_tab" text="Game">
                    <content>
                        <ListView fx:id="loglist_game" minHeight="-Infinity" prefWidth="267.0" />
                    </content>
                </Tab>
                <Tab fx:id="messages_tab" text="Messages">
                    <content>
                        <ListView fx:id="loglist_messages" prefWidth="267.0" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
      <AnchorPane minHeight="45.0" prefWidth="269.0" AnchorPane.bottomAnchor="0.0">
         <children>
              <TextField fx:id="messageField" layoutX="14.0" layoutY="9.0" prefHeight="25.0" prefWidth="169.0" promptText="Message Player" />
              <Button fx:id="sendButton" layoutX="205.0" layoutY="9.0" mnemonicParsing="false" text="Send" />
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
